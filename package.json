{
  "name": "resume-toolkit",
  "version": "0.1.0",
  "description": "Complete Job Application Workflow System with ATS scoring, PDF parsing, and job tracking",
  "type": "module",
  "bin": {
    "resume-toolkit": "./dist/cli/index.js"
  },
  "scripts": {
    "dev": "tsx src/cli/index.ts",
    "build": "tsc",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:coverage:check": "vitest --coverage && node scripts/check-coverage.js",
    "lint": "eslint src --ext .ts,.tsx --ignore-pattern '**/*.test.ts' --ignore-pattern '**/__tests__/**'",
    "lint:fix": "eslint src --ext .ts,.tsx --fix --ignore-pattern '**/*.test.ts' --ignore-pattern '**/__tests__/**'",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json}\"",
    "clean": "rm -rf dist coverage .vitest node_modules/.cache",
    "prepare": "husky",
    "pre-commit": "pre-commit run --all-files",
    "quality-gates": "npm run lint && npm run type-check && npm run test:coverage:check && echo 'âœ… All quality gates passed!'"
  },
  "keywords": [
    "resume",
    "ats",
    "job-search",
    "cli",
    "toolkit"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@inquirer/prompts": "^7.9.0",
    "@libsql/client": "^0.14.0",
    "axios": "^1.12.2",
    "chalk": "^5.3.0",
    "cheerio": "^1.1.2",
    "commander": "^12.1.0",
    "inquirer": "^12.10.0",
    "js-yaml": "^4.1.0",
    "ora": "^8.1.0",
    "playwright": "^1.48.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.9",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.8.6",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "@vitest/coverage-v8": "^2.1.4",
    "@vitest/ui": "^2.1.4",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "husky": "^9.1.7",
    "prettier": "^3.3.3",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  }
}
